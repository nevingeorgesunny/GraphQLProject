type Query {
    bookById(id: ID): Book
    bookById2(id: ID): Book
    bookByIdFilter(filter: String,pagination : String): Book
    bookByIdFilterWithObject(filter: BookFilter): Book
}

type Book {
    id: ID
    name: String
    pageCount: Int
    author: Author
}

input BookFilter {
    id: String
    name: String
    pageCount: Int
    someUUID: String
}


type Author {
    id: ID
    firstName: String
    lastName: String
}
